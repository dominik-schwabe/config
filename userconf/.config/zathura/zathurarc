set selection-clipboard clipboard
#set recolor
set default-bg "#111111"
set default-fg "#dcdccc"
set recolor-lightcolor "#111111"
set recolor-darkcolor "#ffffff"
set highlight-color "#00FF00"
set highlight-active-color "#00AA00"
set highlight-transparency 0.5
set synctex true
set synctex-editor-command "code -g %{input}:%{line}"
set scroll-step 150
