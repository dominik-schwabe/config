[user]
	email = schwabedominik@gmail.com
	name = Dominik Schwabe
[protocol "file"]
	allow = always
[init]
	defaultBranch = main
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[mergetool "nvimdiff4"]
	cmd = nvim -d $LOCAL $BASE $REMOTE $MERGED -c '$windcmd w' -c 'windcmd J'
